{
  "manifest_version": 3,
  "name": "Manhwa tracker",
  "version": "3.0.0",
  "description": "Manhwa Tracker for a few sites",
  "action": {
    "default_title": "Manhwa Tracker",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/ContentScript.page.tsx-loader-eb19b3fe.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/icon128-0022192c.png",
        "assets/emotion-styled.browser.esm-f5ef74c8.js",
        "assets/ContentScript.page.tsx-0fb2d34f.js"
      ],
      "use_dynamic_url": true
    }
  ]
}