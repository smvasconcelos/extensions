{
  "manifest_version": 3,
  "name": "Manhwa_tracker",
  "version": "0.9.0",
  "description": "My Chrome Extension",
  "icons": {
    "16": "assets/icon128.png",
    "48": "assets/icon128.png",
    "128": "assets/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Manhwa_tracker",
    "default_popup": "popup.html"
  },
  "permissions": [
		"storage"
	],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": [
        "scripts/jquery.min.js",
        "scripts/ajax.min.js",
        "contentScript.js"
      ],
      "css": ["css/style.css"]
    }
  ]
}
